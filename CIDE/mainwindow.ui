      <property name="styleSheet">
       <string notr="true">/* Tab 内容区，添加背景图和透明度 */




/* TabBar 上的每个 tab */
QTabBar::tab {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #e0e0e0, stop:1 #c8c8c8);
    color: #333;
    padding: 8px 18px;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
    margin-right: 2px;
}

/* 鼠标悬停效果 */
QTabBar::tab:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #d0d0d0, stop:1 #b8b8b8);
}

/* 选中状态的 tab */
QTabBar::tab:selected {
    background: #f0f0f0;
    border-bottom: 3px solid #4CAF50;
    color: #000;
}

/* 未选中状态 tab 的上边框微调 */
QTabBar::tab:!selected {
    margin-top: 3px;
}

/* TabBar 默认靠左排列 */
QTabWidget::tab-bar {
    alignment: left;
}

</string>
